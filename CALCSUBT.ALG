Algoritmo "CalcSubt"

Var

   num_1, num_2, result: inteiro
Inicio

      escreval("Calculadora de subtração")
      escreval("Informe o primeiro número: ")
      leia(num_1)
      escreval("Informe o segundo número: ")
      leia(num_2)
      result <- num_1 - num_2
      escreval("O resultado da subtração é: ", result)


Fimalgoritmo